<?xml version="1.0" encoding="utf-8"?>
<selector
	xmlns:android="http://schemas.android.com/apk/res/android">

	<item android:state_enabled="false">
		<shape
			xmlns:android="http://schemas.android.com/apk/res/android"
			android:shape="rectangle">
			<gradient
				android:startColor="#C4D3D9"
				android:endColor="#C4D3D9"
				android:angle="270" />
			<stroke
				android:width="0.5dp"
				android:color="#88AAAAAA" />
			<corners
				android:radius="0dp" />
			<padding
				android:left="5dp"
				android:top="5dp"
				android:right="5dp"
				android:bottom="5dp" />
			<size
				android:height="30dp"
				android:width="30dp" />
		</shape>
	</item>
	<item android:state_pressed="true" >
		<shape
			xmlns:android="http://schemas.android.com/apk/res/android"
			android:shape="rectangle">
			<gradient
				android:startColor="#C4D3D9"
				android:endColor="#C4D3D9"
				android:angle="270" />
			<stroke
				android:width="0.5dp"
				android:color="#88AAAAAA" />
			<corners
				android:radius="0dp" />
			<padding
				android:left="5dp"
				android:top="5dp"
				android:right="5dp"
				android:bottom="5dp" />
			<size
				android:height="30dp"
				android:width="30dp" />
		</shape>
	</item>

	<item android:state_pressed="false">
		<shape
			xmlns:android="http://schemas.android.com/apk/res/android"
			android:shape="rectangle">
			<gradient
				android:startColor="#D4D4D1"
				android:endColor="#D4D4D1"
				android:angle="270" />
			<stroke
				android:width="0.5dp"
				android:color="#88AAAAAA" />
			<corners
				android:radius="0dp" />
			<padding
				android:left="5dp"
				android:top="5dp"
				android:right="5dp"
				android:bottom="5dp" />
			<size
				android:height="30dp"
				android:width="30dp" />
		</shape>
	</item>
</selector>